package com.acorn;

import java.util.Scanner;

public class Run2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("첫 번째 정수를 입력해주세요");
		int fN = sc.nextInt();
		System.out.println("두 번째 정수를 입력해주세요");
		int sN = sc.nextInt();
		System.out.println("첫 번째정수: "+fN+"\n두 번째정수: "+sN+"\n더하기 결과: "+(fN+sN)+"\n빼기 결과: "+(fN-sN)+"\n곱하기 결과: "+(fN*sN)+"\n나누기 결과: "+(fN/sN));
		sc.close();
	}
}
